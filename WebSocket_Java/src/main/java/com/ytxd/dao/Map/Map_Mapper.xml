<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="com.ytxd.dao.Map.Map_Mapper">

	<select id="getRiverMsg" resultType="java.util.Map">
		select * from st_river_info where fid =#{fid}
	</select>
	<select id="getRsvrMsg" resultType="java.util.Map">
		select * from st_rsvr_info where fid =#{fid}
	</select>
</mapper>